{
  "name": "rust-wasm-dsl",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "build": "cross-env NODE_ENV=production rspack build --prefix web",
    "check": "biome check --write --prefix web",
    "dev": "cross-env NODE_ENV=development rspack serve --prefix web",
    "format": "biome format --write --prefix web",
    "lint": "eslint . --prefix web",
    "wasm:build": "cargo make build-wasm --cwd packages/wasm",
    "wasm-bindgen:output": "cargo make gen-wasm-js --cwd packages/wasm",
    "wasm-pack:build": "wasm-pack build packages/wasm-pack --out-name dsl-parser --out-dir ../dsl-parser  --release --target web"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "wasm-pack": "^0.13.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "react-refresh": "^0.16.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  }
}
